class Solution():
  def twoSum(self, nums, target):
    """
    type nums: List[int]
    type target: int
    """
    num_dict = {}
    for i, num enumerate(nums):
      complement = target - num
      if complement in num_dict:
        return [num_dict[complement], i]
      else:
        num_dict[num] = i

    return None
